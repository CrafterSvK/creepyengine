<!--
world.xml
List of world biomes that are in the world

Legend:
"id" - Id of biome [Refers to random world generator]
"name" - Name of biome
"region-type" - Type of region [Directly refers to "region" tag in entities]
"type" - Type of biome (usually refers to property of that biome)

-->
<biomes>
    <biome>
        <name>Plains</name>
        <id>1</id>
        <region-type>plains</region-type>
        <type>mild</type>
        <type>bottom-water</type>
    </biome>
    <biome>
        <name>Desert</name>
        <id>2</id>
        <region-type>desert</region-type>
        <type>hot</type>
    </biome>
    <biome>
        <name>Mountains</name>
        <id>3</id>
        <region-type>mountains</region-type>
        <type>colder</type>
        <type>windy</type>
    </biome>
    <biome>
        <name>Tundra</name>
        <id>4</id>
        <region-type>tundra</region-type>
        <type>cold</type>
    </biome>
</biomes>